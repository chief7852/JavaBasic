package subj_game;

public class ex {

	static void delay(int i){
		long end = System.currentTimeMillis();
		long start = System.currentTimeMillis();
		while((end-start)<i)
			end=System.currentTimeMillis();
	}
	public static void main(String[] args) {
		ex a = new ex();
			for(int i=0; i<15; i++){
				long end = System.currentTimeMillis();
				long start = System.currentTimeMillis();
				while((end-start)<200)
					end=System.currentTimeMillis();
			}
			System.out.print(".");
		}		
	}


