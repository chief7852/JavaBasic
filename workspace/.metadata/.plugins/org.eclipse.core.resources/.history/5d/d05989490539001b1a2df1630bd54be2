package Mandoo;

public class ex
{

	public static void main(String[] args) 
	{	for(int i = 0 ; i <2000; i++){
		long end = System.currentTimeMillis();
		long start = System.currentTimeMillis();
		while((end-start)<100)
			end=System.currentTimeMillis();
		String a = "고향만두";
		System.out.println("\t\t\t\t\t\t\t"+a);
		}
	}
}
