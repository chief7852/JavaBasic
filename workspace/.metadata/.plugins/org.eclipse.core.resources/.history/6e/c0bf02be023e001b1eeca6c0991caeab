package programers;

import java.lang.reflect.Array;
import java.util.ArrayList;



public class decimal {
	public static void main(String[] args) {
		int n = 5;
		int answer = 0;
		ArrayList<Integer> ang = new ArrayList<>();
		int temp =0;
		for(int i = 2; i <= n; i++)
		{
			temp = 0;
			for(int j = 2; j <= n;j++)
			{
				if(i >= j )
				{
					if(i%j==0)
					{
						temp++;
					}
				}
			}
			ang.add(temp);
		}
		System.out.println(ang);
		
		for(int i = 0 ; i <ang.size();i++)
		{
			if(ang.get(i)==1)
			{
				answer++;
			}
		}
	}
}