package programers;

public class sum {

	public static void main(String[] args) {
		int a=5;
		int b=24;
		int sum = 0;
		String day[] = {"FRI", "SAT", "SUN", "MON", "TUE", "WED", "THU"};
		int m[] = {31,29,31,30,31,30,31,31,30,31,30,31};
		
		for(int i = 0 ;i<a-1;i++ ){
			sum += m[i];
		}
		sum +=b-1;
		String answer = day[sum%7];
		System.out.println(answer);
		
	}

}
