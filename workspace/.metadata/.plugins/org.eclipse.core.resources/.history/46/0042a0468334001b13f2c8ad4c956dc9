package home;

public class machine_inside {

	
	int money = 0;
	String button1000= "◎";
	String button1500= "◎";
	String button2000= "◎";
	void money(){
		 if(this.money >=2000){
			 button2000 = "●";
			 if(this.money >=1500){
				 button1500 = "●";
				 if(this.money >=1000){
					 button1000 = "●";
				 }
			 }
		 }
	}
	
}
