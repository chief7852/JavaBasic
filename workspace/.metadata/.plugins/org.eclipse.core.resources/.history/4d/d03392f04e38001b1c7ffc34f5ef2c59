	String leek = "대파 ";
	String paste = "고추장 ";
	String mayo = "마요네즈 ";
	String ongs = "옹스 ";
	String dump = "1.돼지고기\t2.두부\t3.양파\t4.양배추\t5.계란\t6.참기름\t7.에이스\t\n8.고추\t9.마늘\t10.치즈\t11.대파\t12.고추장\t13.마요네즈\t14.옹스";
	void dump(){
		System.out.println(dump);
	}
	// 반죽재료
	String rice = "쌀가루";
