package programers;

public class sum {

	public static void main(String[] args) {
		int a;
		int b;
		a = 3;
		b = 5;
		long answer = 0;
		 if(a > b){
	            for(int i = a; i <=b; i++)
			{
				answer +=i;
			}
	                }
	        else if(b > a)
	        {
	            for(int i = b; i <= a; i++)
			{
				answer +=i;
			}
	        }
	        else{answer = a;}
		System.out.println(answer);
	}

}
