package d_array;

import java.util.Arrays;

public class Subject2 {

	public static void main(String[] args) {
		// 나누어 떨어지는 숫자 배열
		int[] arr = new int[100];
		for(int i = 0; i < arr.length; i++){
				arr[i] = (int)(Math.random() * 100)+1;
		}
		
		
		/*
		 * 2~5 사이의 랜덤한 수로 나누어 떨어지는 숫자로만 이루어진 배열을 정렬하여 출력해주세요.
		 * 5
		 * [5,10,15,20,25...]
		 */
	
	}

}
