package programers;

public class Kim {

	public static void main(String[] args) {
		String[] seoul = {"Jane", "Kim"};
		String temp = "김서방은";
		int i = 0;
		while(true){
			if(seoul[i].equals("Kim")){
				temp +=seoul[i];
				System.out.println("김서방은"+(i-1)+"에 있다");
				break;
			}
			i++;
		}

	}

}
