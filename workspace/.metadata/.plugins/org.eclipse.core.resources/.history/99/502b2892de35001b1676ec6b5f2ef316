package subj_game;

public class ex {

	void delay(int i){
		long end = System.currentTimeMillis();
		long start = System.currentTimeMillis();
		while((end-start)<i)
			end=System.currentTimeMillis();
	}
	public static void main(String[] args) {
		void play(){
			for(int i=0; i<45; i++){
				this.wait(20);
				System.out.print("â– ");
			}
		}		
	}


}