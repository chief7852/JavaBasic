package programers;

public class sum {

	public static void main(String[] args) {
		int a;
		int b;
		int sum = 0;
		String month[] = {"SUN","MON","TUE","WED","THU","FRI","SAT"};
		int m[] = {31,29,31,30,31,30,31,31,30,31,30,31};
		
		for(int i = 0 ;i<m.length;i++ ){
			sum += m[i];
		}
		System.out.println(sum);
		
	}

}
