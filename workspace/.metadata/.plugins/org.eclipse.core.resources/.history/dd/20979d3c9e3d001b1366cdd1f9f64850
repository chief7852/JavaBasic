package programers;

class Solution {
    public int solution(int n) {
        int answer = 0;
        for(int i = 1; i <= n;i++)
        {
        	for(int j = 1; j <= n ;j++)
        	{
        		if(i != 1 && i >j)
        		{
        			if(i%j == 0)
        			{
        				answer++;
        			}
        		}
        	}
        }
        return answer;
    }
}