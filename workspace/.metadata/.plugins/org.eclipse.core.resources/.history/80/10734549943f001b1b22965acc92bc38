package programers;

import java.util.HashMap;
import java.util.Set;

public class sportw {
	public static void main(String[] args)
	{
		int n =5;
		int[] reserve = {1};
		int[] lost = {3};
		
		int[] all = new int[n];

		for (int i : reserve)
			all[i - 1]++;

		for (int i : lost)
			all[i - 1]--;

		for (int i = 0; i < all.length; i++)
			if (all[i] < 0)
				if (i != all.length - 1 && all[i + 1] > 0) {
					all[i]++;
					all[i + 1]--;
				} else if (i != 0 && all[i - 1] > 0) {
					all[i]++;
					all[i - 1]--;
				}

		int answer = 0;

		for (int i = 0; i < all.length; i++)
			if (!(all[i] < 0))
				answer++;
		System.out.println(answer);
		
	}
}
