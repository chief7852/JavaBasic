package programers;

import java.util.HashMap;
import java.util.Set;

public class sportw {
	public static void main(String[] args)
	{
		int n =5;
		int[] reseve = {1,3,5};
		int[] lost = {2,4};
		
		int[] students = new int[n];
		int x = 0;
		for(int i = 0; i <students.length; i++)
		{
			students[i] = 1;
		}
		
		
		
		for(int i = 0; i <students.length;i++)
		{
			for(int j = 0; j <reseve.length; j++)
			{
				if(reseve[j]== i+1)
				{
					students[i] += 1;
					
				}
			}
			for(int j = 0;j <lost.length;j++)
			{
				if(lost[j] == i+1)
				{
					students[i] -= 1;
				}
			}
			
				x +=students[i];
			if(x>n)
			{
				x=n;
			}
		}
		
		System.out.println(x);
		
	}
}
